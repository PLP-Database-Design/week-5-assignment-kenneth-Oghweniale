<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Hospital Dashboard - Providers</title>
    <link rel="stylesheet" href="/css/styles.css" />
  </head>
  <body>
    <div class="container">
      <h1>Providers List</h1>
      <table class="table">
        <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Specialty</th>
          </tr>
        </thead>
        <tbody>
          <% providers.forEach(provider => { %>
          <tr>
            <td><%= provider.first_name %></td>
            <td><%= provider.last_name %></td>
            <td><%= provider.provider_specialty %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>

      
        </tbody>
      </table>
    </div>
  </body>
</html>
